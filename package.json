{
  "name": "invoke-installer",
  "productName": "invoke-installer",
  "version": "1.0.0",
  "description": "Invoke Community Edition",
  "main": ".vite/build/main.js",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "app:dir": "npm run package && electron-builder --dir",
    "app:dist": "npm run package && electron-builder",
    "lint:knip": "knip --tags=-knipignore",
    "lint:dpdm": "dpdm --no-warning --no-tree --transform --exit-code circular:1 src/main/index.ts src/renderer/index.ts src/preload/index.ts",
    "lint:eslint": "eslint --max-warnings=0 .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "lint": "concurrently -g -c red,green,yellow,blue,magenta npm:lint:*",
    "fix": "eslint --fix . && prettier --log-level warn --write .",
    "test": "npm rebuild node-pty && vitest",
    "test:ui": "npm rebuild node-pty && vitest --coverage --ui",
    "test:no-watch": "npm rebuild node-pty && vitest --no-watch",
    "rebuild": "electron-rebuild -f -w node-pty",
    "postinstall": "npm run rebuild",
    "react-devtools": "react-devtools"
  },
  "postinstall": "electron-builder install-app-deps",
  "devDependencies": {
    "@electron-forge/cli": "^7.6.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.0",
    "@electron-forge/plugin-fuses": "^7.6.0",
    "@electron-forge/plugin-vite": "^7.6.0",
    "@electron/fuses": "^1.8.0",
    "@invoke-ai/eslint-config-react": "^0.0.14",
    "@invoke-ai/prettier-config-react": "^0.0.7",
    "@types/ip": "^1.1.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "concurrently": "^9.1.0",
    "dpdm": "^3.14.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-path": "^1.4.0-beta.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.13.0",
    "jsdom": "^25.0.1",
    "knip": "^5.39.1",
    "prettier": "^3.4.1",
    "react-devtools": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^5.4.11",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "keywords": [],
  "author": {
    "name": "Invoke AI, Inc.",
    "email": "info@invoke.com"
  },
  "license": "MIT",
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/typed-ipc": "^1.0.2",
    "@fontsource-variable/inter": "^5.1.0",
    "@invoke-ai/ui-library": "^0.0.44",
    "@nanostores/react": "^0.8.2",
    "@renovatebot/pep440": "^4.0.1",
    "@types/lodash-es": "^4.17.12",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "ansi-regex": "^6.1.0",
    "electron-context-menu": "^4.0.4",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.0",
    "ip": "^2.0.1",
    "linkify-react": "^4.2.0",
    "linkifyjs": "^4.2.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.9",
    "nanostores": "^0.11.3",
    "node-pty": "^1.1.0-beta27",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-hotkeys-hook": "^4.6.1",
    "react-icons": "^5.4.0",
    "serialize-error": "^11.0.3",
    "strip-ansi": "^7.1.0",
    "tsafe": "^1.8.5",
    "use-stick-to-bottom": "^1.0.42"
  },
  "build": {
    "appId": "com.invoke.installer",
    "productName": "Invoke-Installer",
    "directories": {
      "output": "dist"
    },
    "files": [
      "package.json",
      ".vite/**/*"
    ],
    "extraResources": [
      {
        "from": "assets/bin",
        "to": "./bin"
      }
    ],
    "linux": {
      "target": "AppImage",
      "artifactName": "${productName}-linux-${arch}.${ext}",
      "icon": "assets/icons/icon.png"
    },
    "mac": {
      "target": {
        "target": "dmg",
        "arch": [
          "arm64"
        ]
      },
      "artifactName": "${productName}-mac-${arch}.${ext}",
      "icon": "assets/icons/icon.icns"
    },
    "win": {
      "target": "portable",
      "artifactName": "${productName}-Windows-${arch}.${ext}",
      "icon": "assets/icons/icon.ico"
    },
    "publish": null
  },
  "peerDependencies": {
    "@emotion/react": "^11.13.5"
  }
}
